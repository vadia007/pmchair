				<field name="jnews_checkboxes" type="list" label="COM_FOXCONTACT_NEWSLETTER_DISPLAY_CHECKBOXES_LBL" description="COM_FOXCONTACT_NEWSLETTER_DISPLAY_CHECKBOXES_DSC" default="hidden|hidden">
